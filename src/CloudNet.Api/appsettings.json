{
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Limits": {
      "MaxRequestBodySize": 104857600
    }
  },
  "Jwt": {
    "Issuer": "CloudNet",
    "Audience": "CloudNet",
    "SigningKey": "",
    "AccessTokenMinutes": 15,
    "RefreshTokenDays": 30,
    "RevokeAllTokensOnReuse": true
  },
  "RateLimiting": {
    "PerUser": {
      "PermitLimit": 60,
      "WindowSeconds": 60,
      "QueueLimit": 0
    },
    "AuthSensitive": {
      "PermitLimit": 5,
      "WindowSeconds": 60,
      "QueueLimit": 0
    },
    "ShareDownload": {
      "PermitLimit": 30,
      "WindowSeconds": 60,
      "QueueLimit": 0
    },
    "InvalidUser": {
      "PermitLimit": 5,
      "WindowSeconds": 60,
      "QueueLimit": 0
    },
    "DefaultRetryAfterSeconds": 10
  },
  "FileStorage": {
    "RootPath": "storage"
  },
  "StorageQuota": {
    "DefaultLimitBytes": 1073741824
  }
}
